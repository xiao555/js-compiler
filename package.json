    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "babel-runtime": "^5.8.0",
    "browser-sync": "^2.12.7",
    "browser-sync-webpack-plugin": "^1.0.1",
    "change-case": "^2.3.1",
    "clean-webpack-plugin": "^0.1.6",
    "copy-loader": "^0.1.3",
